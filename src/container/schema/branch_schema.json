{
    "title":"branch",
    "type": "object",
    "required": ["result","type","info"],
    "properties": {
        "result": { "title":"Result","type": "string"},
        "type": {
            "type": "string",
            "title": "Job Type",
            "default": "branch",
            "enum": ["database", "http","branch","control_message","control_tag_message","random","calculator"]
        },
        "info": {
            "title":"Info",
            "type": "object",
            "required": ["condition"],
            "properties": {
                "condition": {
                    "type": "array",
                    "title": "Branch Condition",
                    "minItems": 1,
                    "items": {
                        "type": "object",
                        "required": ["key","value","exp"],
                        "properties": {
                            "key": { "type": "string", "default": "" },
                            "value": { "type": "string", "default": "" },
                            "exp": { "type": "string", "enum": ["eq","ne","lt","le","gt","ge","in","null","notNull"] }
                        }
                    }
                }
            }
        }
    }
}