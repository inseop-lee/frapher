{
    "title":"internal",
    "type": "object",
    "required": ["result","type","source","info"],
    "properties": {
        "result": { "type": "string" },
        "type": {
            "type": "string",
            "title": "Job Type",
            "default":"database",
            "enum": ["database", "http","branch","control_message","control_tag_message","random","calculator"]
        },
        "source": { "type": "string" },
        "info": {
            "type": "object",
            "required": ["fields","condition"],
            "properties": {
                "fields":{
                    "type":"array",
                    "title":"Internal table name",
                    "items":{ "type" : "string" }
                },
                "condition": {
                    "type": "array",
                    "title": "Filter query",
                    "minItems": 1,
                    "items": {
                        "type": "object",
                        "required": ["key","value","exp"],
                        "properties": {
                            "key": { "type": "string", "default": "" },
                            "value": { "type": "string", "default": "" },
                            "exp": { "type": "string", "enum": ["eq","ne","lt","le","gt","ge","in","null","notNull"] }
                        }
                    }
                }
            }
        }
    }
}